export OS_NAME=$(uname -s)

# ---------------------------------------------------------------------------- #
# Mac OS X - Darwin                                                            #
# ---------------------------------------------------------------------------- #

if [ "${OS_NAME}" = "Darwin" ] ; then
  [ -f $HOME/.runcom/profile ] && source $HOME/.runcom/profile
  [ -f $HOME/.runcom/setup ] && source $HOME/.runcom/setup
  [ -f $HOME/.runcom/functions ] && source $HOME/.runcom/functions
  [ -f $HOME/.runcom/aliases ] && source $HOME/.runcom/aliases
fi
